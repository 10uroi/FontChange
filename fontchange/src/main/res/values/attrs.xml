<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FTextView">
        <attr name="fontTextView" format="string"/>
        <attr name="fontStyleTextView" format="enum">
            <enum name="normal" value="0"/>
            <enum name="italic" value="1"/>
            <enum name="bold" value="2"/>
            <enum name="bold_italic" value="3"/>
        </attr>
        <attr name="fontDefaultTextView" format="enum">
            <enum name="asd" value="0"/>
            <enum name="qwe" value="1"/>
            <enum name="ert" value="2"/>
            <enum name="rty" value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="FButton">
        <attr name="fontButton" format="string"/>
        <attr name="fontStyleButton" format="enum">
            <enum name="normal" value="0"/>
            <enum name="italic" value="1"/>
            <enum name="bold" value="2"/>
            <enum name="bold_italic" value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="FEditText">
        <attr name="fontEditText" format="string"/>
        <attr name="fontStyleEditText" format="enum">
            <enum name="normal" value="0"/>
            <enum name="italic" value="1"/>
            <enum name="bold" value="2"/>
            <enum name="bold_italic" value="3"/>
        </attr>
    </declare-styleable>
</resources>